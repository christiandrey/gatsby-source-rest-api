{
  "name": "gatsby-source-rest-api",
  "version": "0.2.2",
  "description": "A Gatsby source plugin for sourcing data into your Gatsby appliction from any REST Api",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir . --ignore __tests__",
    "prepare": "cross-env NODE_ENV=production npm run build"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source",
    "gatsby-source-plugin",
    "gatsby-source-rest-api",
    "gatsby-rest-api-plugin",
    "gatsby-rest-api",
    "rest-api"
  ],
  "author": "Dan Spratling <danspratling@hotmail.co.uk>",
  "license": "MIT",
  "dependencies": {
    "fetch-multiple": "^1.2.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-preset-minify": "^0.5.0",
    "cross-env": "^5.2.0"
  }
}
